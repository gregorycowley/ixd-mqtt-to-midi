{
  "name": "ixd-mqtt-to-midi",
  "version": "0.2.1",
  "description": "A simple Electron app to convert MQTT messages to MIDI messages",
  "main": "src/main/main.js",
  "type": "commonjs",
  "scripts": {
    "start": "electron --trace-warnings .",
    "build": "electron-forge start",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "test": "mocha",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "make": "electron-forge make"
  },
  "author": "Gregory Cowley",
  "repository": {
    "type": "git",
    "url": "https://github.com/gregorycowley/ixd-mqtt-to-midi.git"},
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.3.0",
    "@electron-forge/maker-deb": "^7.3.0",
    "@electron-forge/maker-rpm": "^7.3.0",
    "@electron-forge/maker-squirrel": "^7.3.0",
    "@electron-forge/maker-zip": "^7.3.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.3.0",
    "electron": "^29.0.1",
    "eslint": "^8.57.0",
    "mocha": "^10.3.0"
  },
  "dependencies": {
    "@material/web": "^1.3.0",
    "easymidi": "^3.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "macaddress-local-machine": "^1.0.7",
    "material": "^0.9.9",
    "material-design-lite": "^1.3.0",
    "material-icons-font": "^2.1.0",
    "mqtt": "^5.3.6"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ixd-mqtt2midi"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
